diff --git a/fs/dax.c b/fs/dax.c
index 91820b9b50b73..2ca33ef5d519d 100644
--- a/fs/dax.c
+++ b/fs/dax.c
@@ -1006,7 +1006,7 @@ int dax_writeback_mapping_range(struct address_space *mapping,
